<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Header of status bar section. -->
	<key>STATUS_BAR</key>
	<string>Thanh Trạng Thái</string>

	<!-- Switch to toggle displaying the unread count in the status bar. -->
	<key>UNREAD_COUNT</key>
	<string>Số lượng chưa đọc</string>

	<!-- Button for choosing from which app the unread notifications count will be shown in the status bar. -->
	<key>SELECT_APPS</key>
	<string>Chọn những ứng dụng</string>

	<!-- Explanation of the unread notifications count button. -->
	<key>UNREAD_COUNT_EXPLANATION</key>
	<string>Hiển thị số thông báo từ "Chọn những ứng dụng" trên thanh trạng thái.</string>

	<!-- Switch to toggle show in foreground. -->
	<key>SHOW_IN_FOREGROUND</key>
	<string>Hiện trên phía sau</string>

	<!-- Explanation of the show in foreground switch. -->
	<key>SHOW_IN_FOREGROUND_EXPLANATION</key>
	<string>Hiển thị thông báo ngay cả khi ứng dụng gửi đang ở phía sau.</string>
</dict>
</plist>
