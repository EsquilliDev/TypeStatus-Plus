<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Header of status bar section. -->
	<key>STATUS_BAR</key>
	<string>Status Bar</string>

	<!-- Switch to toggle displaying the unread count in the status bar. -->
	<key>UNREAD_COUNT</key>
	<string>Jumlah Belum Terbaca</string>

	<!-- Button for choosing from which app the unread notifications count will be shown in the status bar. -->
	<key>SELECT_APPS</key>
	<string>Pilih Aplikasi</string>

	<!-- Explanation of the unread notifications count button. -->
	<key>UNREAD_COUNT_EXPLANATION</key>
	<string>Memperlihatkan jumlah notifikasi belum terbaca dari aplikasi yang terpilih di status bar</string>

	<!-- Switch to toggle show in foreground. -->
	<key>SHOW_IN_FOREGROUND</key>
	<string>Perlihatkan di latar depan</string>

	<!-- Explanation of the show in foreground switch. -->
	<key>SHOW_IN_FOREGROUND_EXPLANATION</key>
	<string>Memperlihatkan notifikasi biarpun aplikasi yang sedang mengirim di latar depan.</string>
</dict>
</plist>
