<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Header of status bar section. -->
	<key>STATUS_BAR</key>
	<string>Stavová lišta</string>

	<!-- Switch to toggle displaying the unread count in the status bar. -->
	<key>UNREAD_COUNT</key>
	<string>Počet neprečítaných</string>

	<!-- Button for choosing from which app the unread notifications count will be shown in the status bar. -->
	<key>SELECT_APPS</key>
	<string>Výber aplikácií</string>

	<!-- Explanation of the unread notifications count button. -->
	<key>UNREAD_COUNT_EXPLANATION</key>
	<string>Zobrazí počet neprečítaných upozornení z vybraných aplikácií v stavovej lište.</string>

	<!-- Switch to toggle show in foreground. -->
	<key>SHOW_IN_FOREGROUND</key>
	<string>Zobraziť v popredí</string>

	<!-- Explanation of the show in foreground switch. -->
	<key>SHOW_IN_FOREGROUND_EXPLANATION</key>
	<string>Zobrazí upozornenia aj keď je istá aplikácia otvorená.</string>
</dict>
</plist>
