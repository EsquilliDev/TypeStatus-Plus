<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<!-- Title. -->
	<key>ALERTS</key>
	<string>Amaran</string>

	<!-- Header for alert type selection. -->
	<key>ALERT_TYPE</key>
	<string>Jenis Amaran</string>

	<!-- Explanation of the alert type setting. -->
	<key>ALERT_TYPE_EXPLANATION</key>
	<string>Yang diatas hanya dipakai untuk notifikasi dari pembekal TypeStatus Plus.</string>

	<!-- Switch to wake the screen when the device is locked. -->
	<key>WAKE_WHEN_LOCKED</key>
	<string>Nyala Bila Terkunci</string>

	<!-- Explanation of the above setting. -->
	<key>WAKE_WHEN_LOCKED_EXPLANATION</key>
	<string>Nyala skrin bila menerima status bar notifikasi TypeStatus bila peranti terkunci. Amaran Pusat Notifikasi sentiasa menyalakan skrin.</string>

	<!-- Header for tap action section. -->
	<key>TAP_TO_OPEN_ACTION</key>
	<string>Tekan</string>

	<!-- Options shown for the tap action. -->
	<key>TAP_TO_OPEN_ACTION_OPEN</key>
	<string>Buka App</string>
	<key>TAP_TO_OPEN_ACTION_DISMISS</key>
	<string>Henti Amaran</string>
	
	<!-- Buttons shown in the segmented controls for the alert type. -->
	<key>OFF</key>
	<string>Padam</string>
	<key>ALERT_TYPE_ICON</key>
	<string>Hanya Ikon</string>
	<key>ALERT_TYPE_OVERLAY</key>
	<string>Bar Status</string>

	<!-- Header for vibration options. -->
	<key>VIBRATIONS</key>
	<string>Getaran</string>

	<!-- Header for notification center options. -->
	<key>NOTIFICATION_CENTER</key>
	<string>Pusat Notifikasi</string>

	<!-- Note displayed below the notification center section. -->
	<key>NOTIFICATION_CENTER_EXPLANATION</key>
	<string>Amaran Pusat Notifikasi mengambil-alih amaran TypeStatus yang biasa.</string>

	<!-- Option to show banners on the lock screen. -->
	<key>ON_LOCK_SCREEN</key>
	<string>Pada Skrin Kekunci</string>

	<!-- Option to show banners on the home screen. -->
	<key>ON_HOME_SCREEN</key>
	<string>Pada Skrin Rumah</string>

	<!-- Option to show banners in apps. -->
	<key>IN_APPS</key>
	<string>Dalam App</string>

	<!-- Explanation of the keep all notifications switch. -->
	<key>KEEP_ALL_BULLETINS_EXPLANATION</key>
	<string>Tunjuk semua amaran TypeStatus dalam pusat notifikasi dan skrin kekunci, atau hanya tunjuk yang terbaru.</string>

	<!-- Button to test the alerts. -->
	<key>TEST_ALERT</key>
	<string>Uji Amaran</string>
</dict>
</plist>
